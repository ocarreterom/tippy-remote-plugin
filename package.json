{
  "name": "tippy-remote-plugin",
  "version": "0.6.0",
  "description": "A Tippy.js plugin to load remote content.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run docs-watch & npm run serve",
    "docs-build": "rollup -c",
    "docs-watch": "rollup -c --watch",
    "lint": "standard --fix",
    "serve": "es-dev-server --app-index docs/index.html --watch --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocarreterom/tippy-remote-plugin.git"
  },
  "keywords": [
    "tippy",
    "tippy.js",
    "popper",
    "popper.js",
    "popover",
    "tooltip"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocarreterom/tippy-remote-plugin/issues"
  },
  "homepage": "https://github.com/ocarreterom/tippy-remote-plugin#readme",
  "dependencies": {},
  "peerDependencies": {
    "popper.js": "^1.0.0",
    "tippy.js": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-eslint": "^10.0.3",
    "es-dev-server": "^2.1.0",
    "popper.js": "^1.16.1",
    "rollup": "^2.36.1",
    "rollup-plugin-replace": "^2.2.0",
    "standard": "^16.0.3",
    "tippy.js": "^5.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "browser"
    ],
    "ignore": [
      "bundle.js"
    ]
  }
}
